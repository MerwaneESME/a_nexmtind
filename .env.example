# Copy to `.env` and fill secrets locally. Do not commit `.env`.

# CORS
AI_CORS_ALLOW_ORIGINS=*

# Supabase (examples)
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# LLM pipeline
LLM_PIPELINE_MODEL=gpt-4o-mini
LLM_PIPELINE_FALLBACK_MODEL=gpt-4o-mini
LLM_PIPELINE_MAX_TOKENS=1500

# RAG
RAG_THRESHOLD=0.60
RAG_TOP_K=3

# Web research (optional, used only after local docs miss)
WEB_RESEARCH_ENABLED=0
WEB_RESEARCH_PROVIDER=tavily
TAVILY_API_KEY=

# Extraction checklist avec LLM (recommandé pour qualité)
CHECKLIST_PDF_USE_LLM_EXTRACTION=1

# OpenAI
OPENAI_API_KEY=

